package Ex_statement;

public class ExSwitch3 {
	public static void main(String[] args) {
		
		char ch = 'B';
		
		switch ( ch ) {
		
		case 'A':
			System.out.println("A를 선택함");
			break;
			
		case 'B':
			System.out.println("B를 선택함");
			break;
			
		case 'C':
			System.out.println("C를 선택함");
			break;

		default:
			break;
		}
		
		
	}
}
